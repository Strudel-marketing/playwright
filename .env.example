# API Keys
API_KEY=your_internal_api_key_here

# Google PageSpeed Insights API
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable PageSpeed Insights API: https://console.cloud.google.com/apis/library/pagespeedonline.googleapis.com
GOOGLE_PAGESPEED_API_KEY=your_google_api_key_here

# Rate Limiting
# Maximum PageSpeed Insights API calls per day (default: 10000)
PSI_DAILY_LIMIT=10000

# Server Configuration
PORT=3000
NODE_ENV=production

# Chrome Configuration (for fallback Lighthouse)
CHROME_PATH=/ms-playwright/chromium-1112/chrome-linux/chrome
